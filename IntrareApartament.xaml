<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Apartamente.IntrareApartament"
             Title="Apartament">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Adauga apartament" Clicked="OnShopAddedClicked" />
    </ContentPage.ToolbarItems>
    <ListView x:Name="VizualizareObiective" Margin="20" ItemSelected="OnListViewItemSelected">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding NumeApartament}" Detail="{Binding Adress}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>